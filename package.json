{
  "name": "@wako-app/mobile-sdk",
  "version": "7.2.1",
  "description": "wako's mobile SDK",
  "main": "dist/public_api.js",
  "typings": "dist/public_api.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/wako-app/mobile-sdk"
  },
  "bugs": {
    "url": "https://github.com/wako-app/mobile-sdk/issues"
  },
  "homepage": "https://github.com/wako-app/mobile-sdk/issues#readme",
  "packageManager": "pnpm@8.1.0",
  "scripts": {
    "start": "tsc -w",
    "export-modules": "ts-node --project ./scripts/tsconfig.json ./scripts/export-all-module.ts",
    "clean": "rm -rf dist",
    "build:builders": "tsc -p builders/tsconfig.builders.json",
    "copy:builders": "cp -r ./builders ./dist",
    "build:lib": "tsc && node ./scripts/add-wako-in-plugin.js",
    "build": "pnpm export-modules && rm -rf dist && pnpm build:lib && rm ./src/public_api.ts && cp ./src/manifest-schema.json ./dist",
    "prepare": "pnpm build && husky install"
  },
  "keywords": [
    "wako",
    "sdk",
    "mobile"
  ],
  "author": "wako",
  "license": "MIT",
  "dependencies": {
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.5",
    "@angular/core": "^15.2.5",
    "@angular/forms": "^15.2.5",
    "@angular/router": "^15.2.5",
    "@capacitor/app-launcher": "^4.1.0",
    "@capacitor/browser": "^4.1.1",
    "@capacitor/core": "^4.7.1",
    "@capacitor/share": "^4.1.1",
    "@ionic/angular": "^7.0.0",
    "@ionic/storage-angular": "^3.0.6",
    "@ngx-translate/core": "^14.0.0",
    "cordova-sqlite-storage": "^6.1.0",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "rxjs": "^7.8.0",
    "systemjs": "^6.14.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "^15.2.4",
    "@types/glob": "^8.1.0",
    "@types/node": "^18.15.11",
    "glob": "^9.3.2",
    "husky": "^8.0.3",
    "karma": "~6.4.1",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "@angular/cdk": "^15.2.5",
    "@angular/common": "^15.2.5",
    "@angular/core": "^15.2.5",
    "@angular/forms": "^15.2.5",
    "@angular/router": "^15.2.5",
    "@capacitor/app-launcher": "^4.1.0",
    "@capacitor/browser": "^4.1.1",
    "@capacitor/core": "^4.7.1",
    "@capacitor/share": "^4.1.1",
    "@ionic/angular": "^7.0.0",
    "@ionic/storage-angular": "^3.0.6",
    "@ngx-translate/core": "^14.0.0",
    "cordova-sqlite-storage": "^6.1.0",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "rxjs": "^7.5.5",
    "systemjs": "^6.12.1",
    "tslib": "^2.3.1"
  }
}
