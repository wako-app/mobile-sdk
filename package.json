{
  "name": "@wako-app/mobile-sdk",
  "version": "10.0.2",
  "description": "wako's mobile SDK",
  "main": "dist/public_api.js",
  "typings": "dist/public_api.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/wako-app/mobile-sdk"
  },
  "bugs": {
    "url": "https://github.com/wako-app/mobile-sdk/issues"
  },
  "homepage": "https://github.com/wako-app/mobile-sdk/issues#readme",
  "packageManager": "pnpm@9.7.1",
  "scripts": {
    "start": "tsc -w",
    "export-modules": "ts-node --project ./scripts/tsconfig.json ./scripts/export-all-module.ts",
    "clean": "rm -rf dist",
    "build:builders": "tsc -p builders/tsconfig.builders.json",
    "copy:builders": "cp -r ./builders ./dist",
    "build:lib": "tsc && node ./scripts/add-wako-in-plugin.js",
    "build": "pnpm export-modules && rm -rf dist && pnpm build:lib && rm ./src/public_api.ts && cp ./src/manifest-schema.json ./dist",
    "prepare": "pnpm build && husky install"
  },
  "keywords": [
    "wako",
    "sdk",
    "mobile"
  ],
  "author": "wako",
  "license": "MIT",
  "dependencies": {
    "@angular/cdk": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@capacitor/app-launcher": "^6.0.2",
    "@capacitor/browser": "^6.0.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/share": "^6.0.2",
    "@ionic/angular": "^8.2.7",
    "@ionic/storage-angular": "^4.0.0",
    "@ngx-translate/core": "^15.0.0",
    "cordova-sqlite-storage": "^6.1.0",
    "ionicons": "^7.4.0",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "rxjs": "^7.8.1",
    "systemjs": "^6.14.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.2.3",
    "glob": "^9.3.2",
    "husky": "^9.1.4",
    "karma": "~6.4.1",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.5.3"
  },
  "peerDependencies": {
    "@angular/cdk": ">=18",
    "@angular/common": ">=18",
    "@angular/core": ">=18",
    "@angular/forms": ">=18",
    "@angular/router": ">=18",
    "@capacitor/app-launcher": ">=6",
    "@capacitor/browser": ">=6",
    "@capacitor/core": ">=6",
    "@capacitor/share": ">=6",
    "@ionic/angular": ">=8",
    "@ionic/storage-angular": ">=4.0.0",
    "@ngx-translate/core": ">=15.0.0",
    "cordova-sqlite-storage": "^6.1.0",
    "ionicons": "^7.4.0",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "rxjs": "^7.8.1",
    "systemjs": "^6.12.1",
    "tslib": "^2.3.1"
  }
}
