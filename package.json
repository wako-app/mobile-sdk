{
  "name": "@wako-app/mobile-sdk",
  "version": "8.0.2",
  "description": "wako's mobile SDK",
  "main": "dist/public_api.js",
  "typings": "dist/public_api.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/wako-app/mobile-sdk"
  },
  "bugs": {
    "url": "https://github.com/wako-app/mobile-sdk/issues"
  },
  "homepage": "https://github.com/wako-app/mobile-sdk/issues#readme",
  "packageManager": "pnpm@8.1.0",
  "scripts": {
    "start": "tsc -w",
    "export-modules": "ts-node --project ./scripts/tsconfig.json ./scripts/export-all-module.ts",
    "clean": "rm -rf dist",
    "build:builders": "tsc -p builders/tsconfig.builders.json",
    "copy:builders": "cp -r ./builders ./dist",
    "build:lib": "tsc && node ./scripts/add-wako-in-plugin.js",
    "build": "pnpm export-modules && rm -rf dist && pnpm build:lib && rm ./src/public_api.ts && cp ./src/manifest-schema.json ./dist",
    "prepare": "pnpm build && husky install"
  },
  "keywords": [
    "wako",
    "sdk",
    "mobile"
  ],
  "author": "wako",
  "license": "MIT",
  "dependencies": {
    "@angular/cdk": "^16.0.1",
    "@angular/common": "^16.0.2",
    "@angular/core": "^16.0.2",
    "@angular/forms": "^16.0.2",
    "@angular/router": "^16.0.2",
    "@capacitor/app-launcher": "^5.0.2",
    "@capacitor/browser": "^5.0.2",
    "@capacitor/core": "^5.0.4",
    "@capacitor/share": "^5.0.2",
    "@ionic/angular": "^7.0.7",
    "@ionic/storage-angular": "^4.0.0",
    "@ngx-translate/core": "^15.0.0",
    "cordova-sqlite-storage": "^6.1.0",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "rxjs": "^7.8.0",
    "systemjs": "^6.14.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
    "@angular/cli": "^16.0.2",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.2.3",
    "glob": "^9.3.2",
    "husky": "^8.0.3",
    "karma": "~6.4.1",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "peerDependencies": {
    "@angular/cdk": ">=16.0.1",
    "@angular/common": ">=16.0.2",
    "@angular/core": ">=16.0.2",
    "@angular/forms": ">=16.0.2",
    "@angular/router": ">=16.0.2",
    "@capacitor/app-launcher": ">=5.0.2",
    "@capacitor/browser": ">=5.0.2",
    "@capacitor/core": ">=5.0.2",
    "@capacitor/share": ">=5.0.2",
    "@ionic/angular": ">=7.0.7",
    "@ionic/storage-angular": ">=4.0.0",
    "@ngx-translate/core": ">=15.0.0",
    "cordova-sqlite-storage": "^6.1.0",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "rxjs": "^7.5.5",
    "systemjs": "^6.12.1",
    "tslib": "^2.3.1"
  }
}
